    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "release-it": "^13.5.1",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "prop-types": "^15.5.7",
    "strip-sourcemap-loader": "0.0.1",
    "tuktuktwo": "1.1.3"
  },
  "peerDependencies": {
    "moment": ">=2.20.0 <3.0.0",
    "moment-timezone": ">=0.5.0 < 1.0.0",
    "pyrene": ">=2.0.0 <3.0.0",
    "react": ">=16.9.0 <17.0.0",
    "react-dom": ">=16.9.0 <17.0.0"
  }
