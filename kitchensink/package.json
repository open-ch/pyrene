{
  "name": "@osag/kitchensink",
  "version": "0.6.0",
  "description": "Pyrene Kitchensink",
  "main": "src/index.jsx",
  "repository": "https://github.com/open-ch/pyrene.git",
  "author": "pyrene@open-systems.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "expose:tuktuktwo": "cd ../tuktuktwo && npm link",
    "expose:pyrene": "cd ../pyrene && npm link",
    "expose:pyrene-graphs": "cd ../pyrene-graphs && npm link",
    "expose:eslint-config": "cd ../eslint-config && npm link",
    "expose:all": "concurrently npm:expose:tuktuktwo npm:expose:pyrene npm:expose:pyrene-graphs npm:expose:eslint-config",
    "link:tuktuktwo": "npm link @osag/eslint-config",
    "link:pyrene": "cd ../pyrene && npm link @osag/eslint-config",
    "link:pyrene-graphs": "cd ../pyrene-graphs && npm link @osag/eslint-config && npm link @osag/pyrene && npm link @osag/tuktuktwo",
    "link:kitchensink": "npm link @osag/eslint-config && npm link @osag/pyrene && npm link @osag/pyrene-graphs",
    "link:all": "concurrently npm:link:tuktuktwo npm:link:pyrene npm:link:pyrene-graphs npm:link:kitchensink",
    "watch:tuktuktwo": "cd ../tuktuktwo && npm run watch",
    "watch:pyrene": "cd ../pyrene && npm run watch",
    "watch:pyrene-graphs": "cd ../pyrene-graphs && npm run watch",
    "watch:kitchensink": "npm run watch",
    "watch:kitchensink-pyrene": "concurrently --kill-others npm:watch:pyrene npm:watch:kitchensink",
    "watch:all": "concurrently --kill-others npm:watch:tuktuktwo npm:watch:pyrene npm:watch:pyrene-graphs npm:watch:kitchensink",
    "develop:all": "npm install && npm run expose:all && npm run link:all && npm run watch:all",
    "develop:pyrene": "npm i --prefix ../pyrene && npm i --prefix .. && npm install && npm run expose:pyrene && npm link @osag/pyrene && npm run watch:kitchensink-pyrene",
    "clean": "rm -rf dist node_modules",
    "prebuild": "npm install",
    "build": "webpack",
    "watch": "webpack serve",
    "watchSourceMap": "SOURCE_MAP=true npm run watch",
    "prelint": "npm install",
    "lint": "eslint --ext .js,.jsx src/",
    "updateIconfont": "bash -c 'cp $0 src/app/data/icons.js;'"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-object-assign": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@osag/eslint-config": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^1.6.2",
    "object-hash": "^2.2.0",
    "postcss": "^8.3.5",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.0",
    "postcss-nesting": "^8.0.1",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.0.0",
    "typescript": "^4.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@osag/pyrene": "3.1.5",
    "@osag/pyrene-graphs": "1.3.1",
    "babel-polyfill": "^6.26.0",
    "clsx": "^1.1.1",
    "markdown-to-jsx": "^6.11.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-syntax-highlighter": "^15.4.3",
    "yup": "^0.32.9"
  }
}
