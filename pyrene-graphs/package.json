    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "prebuild": "npm run clean && npm install",
    "build": "webpack && NODE_ENV=production webpack",
    "build-debug": "NODE_ENV=debug webpack",
    "watch": "webpack -w",
    "expose": "npm link && npm run watch",
    "expose-tuktuktwo": "npm link && npm link tuktuktwo && npm run watch",
    "pretest": "npm install",
    "test": "jest",
    "testWatch": "jest --watch",
    "test:bamboo": "NODE_ENV=test JEST_FILE=test.json && jest --config=./jest.bamboo.config.js",
    "prerelease": "npm run build && npm run test",
    "release": "release-it",
    "prelint": "npm install",
    "lint": "eslint --ext .js,.jsx src/"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/register": "^7.9.0",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.0.0",
    "babel-plugin-react-docgen": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
