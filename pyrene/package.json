{
  "name": "@osag/pyrene",
  "version": "2.14.0",
  "description": "Open Systems Component Library",
  "main": "dist/pyrene.js",
  "style": "dist/pyrene.css",
  "repository": "https://github.com/open-ch/pyrene.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "pyrene@open-systems.com",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "prebuild": "npm ci",
    "build": "webpack && NODE_ENV=production webpack && npm run build-ts-declarations",
    "build-debug": "NODE_ENV=debug webpack",
    "watch": "webpack -w",
    "expose": "npm link && npm run watch",
    "test": "jest",
    "build-ts-declarations": "tsc --emitDeclarationOnly true --noEmit false",
    "coverage": "jest --coverage",
    "testWatch": "jest --watch",
    "test:bamboo": "NODE_ENV=test JEST_FILE=test.json && jest --config=./jest.bamboo.config.js",
    "prerelease": "npm install",
    "release": "release-it",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx src",
    "updateIconfont": "bash -c 'cat $0 | fgrep -A3 .icon- | sed -E '\"'\"' s/\\.icon-(.*):before /:global(.pyreneIcon-\\1:before)/ '\"'\"' >  src/styles/iconfont.css; cp $1 src/fonts/pyrene-iconfont.woff;'"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@osag/eslint-config": "^1.6.0",
    "@release-it/bumper": "^2.0.0",
    "@svgr/webpack": "^5.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.9.50",
    "@types/react-select": "^3.0.28",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-docgen": "^4.2.1",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^27.0.3",
    "jest-bamboo-formatter": "^1.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "postcss-nesting": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.8",
    "release-it": "^14.7.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.1.4",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react-select": "^3.1.1",
    "react-table": "^6.8.6",
    "react-tiny-popover": "^4.0.0",
    "react-window": "^1.8.5",
    "strip-sourcemap-loader": "0.0.1",
    "uniqid": "^5.0.3"
  },
  "peerDependencies": {
    "react": ">=16.9.0 <18.0.0",
    "react-dom": ">=16.9.0 <18.0.0"
  }
}
