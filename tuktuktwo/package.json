    "@babel/preset-react": "^7.9.1",
    "@babel/register": "^7.9.0",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.14.0",
