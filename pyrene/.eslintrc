{
  "extends": "@osag/eslint-config",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-imports": [
      "error",
      "jquery"
    ],
    "react/jsx-props-no-spreading": "off", 
    "react/forbid-prop-types": 0, 
    "no-multiple-empty-lines": 0,
    "@typescript-eslint/unbound-method": 0
  },
  "overrides": [
    {
        "files": ["**/*.tsx"],
        "rules": {
            "react/prop-types": "off"
        }
    }
 ]
}
