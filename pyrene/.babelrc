{
    "presets": [
      "env",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          ["react-css-modules", {
            "generateScopedName": "[local]"
          }],
        ]
      },
      "development": {
        "plugins": [
          ["react-css-modules", {
            "generateScopedName": "[name]__[local]--[hash:base64:10]"
          }],
          "react-docgen",
        ]
      },
      "production": {
        "plugins": [
          ["react-css-modules", {
            "generateScopedName": "[name]__[local]--[hash:base64:10]"
          }],
          "react-docgen",
        ]
      }
    },




    "plugins": [
      "transform-object-assign",
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
}
