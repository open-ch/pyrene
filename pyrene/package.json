{
  "name": "pyrene",
  "version": "2.2.9",
  "description": "Open Systems Component Library",
  "main": "dist/pyrene.js",
  "style": "dist/pyrene.css",
  "repository": "https://github.com/open-ch/pyrene.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "pyrene@open-systems.com",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "prebuild": "npm run clean && npm install",
    "build": "webpack && NODE_ENV=production webpack",
    "build-debug": "NODE_ENV=debug webpack",
    "watch": "webpack -w",
    "expose": "npm link && npm run watch",
    "test": "jest",
    "check-types": "tsc",
    "coverage": "jest --coverage",
    "testWatch": "jest --watch",
    "test:bamboo": "NODE_ENV=test JEST_FILE=test.json && jest --config=./jest.bamboo.config.js",
    "prerelease": "npm run build && npm run test",
    "release": "release-it",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx src && npm run check-types",
    "updateIconfont": "bash -c 'cat $0 | fgrep -A3 .icon- | sed -E '\"'\"' s/\\.icon-(.*):before /:global(.pyreneIcon-\\1:before)/ '\"'\"' >  src/styles/iconfont.css; cp $1 src/fonts/pyrene-iconfont.woff;'"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.36",
    "@types/react-css-modules": "^4.6.2",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.0.0",
    "babel-plugin-react-docgen": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-pyrene": "^1.3.6",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^25.1.0",
    "jest-bamboo-formatter": "^1.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.27",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.0",
    "raw-loader": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "release-it": "^13.1.2",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react-select": "^3.1.0",
    "react-svg-inline": "^2.1.1",
    "react-table": "^6.8.6",
    "react-tiny-popover": "^4.0.0",
    "react-window": "^1.8.5",
    "strip-sourcemap-loader": "0.0.1",
    "uniqid": "^5.0.3"
  },
  "peerDependencies": {
    "react": ">=16.9.0 <17.0.0",
    "react-dom": ">=16.9.0 <17.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26"
  }
}
