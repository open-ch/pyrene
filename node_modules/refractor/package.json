      "lang/*.js"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "toc",
        {
          "heading": "contents",
          "skip": "license"
        }
      ],
      "./script/count",
      "./script/support"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooorm/refractor.git"
  },
  "scripts": {
    "build": "npm run build-bundle && npm run build-mangle",
    "build-bundle": "browserify index.js -s refractor > refractor.js",
    "build-mangle": "browserify index.js -s refractor -p tinyify > refractor.min.js",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "generate": "node script/languages && node script/list",
    "test": "npm run generate && npm run format && npm run build && npm run test-coverage",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js"
  },
  "version": "3.5.0",
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "import/extensions": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prefer-includes": "off",
      "unicorn/prefer-optional-catch-binding": "off",
      "unicorn/prefer-type-error": "off",
      "unicorn/string-content": "off",
      "guard-for-in": "off"
