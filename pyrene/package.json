{
  "name": "pyrene",
  "version": "2.3.1",
  "description": "Open Systems Component Library",
  "main": "dist/pyrene.js",
  "style": "dist/pyrene.css",
  "repository": "https://github.com/open-ch/pyrene.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "pyrene@open-systems.com",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "prebuild": "npm run clean && npm install",
    "build": "webpack && NODE_ENV=production webpack && npm run build-ts-declarations",
    "build-debug": "NODE_ENV=debug webpack",
    "watch": "webpack -w",
    "expose": "npm link && npm run watch",
    "test": "jest",
    "build-ts-declarations": "tsc --emitDeclarationOnly true --noEmit false",
    "coverage": "jest --coverage",
    "testWatch": "jest --watch",
    "test:bamboo": "NODE_ENV=test JEST_FILE=test.json && jest --config=./jest.bamboo.config.js",
    "prerelease": "npm run build && npm run test",
    "release": "release-it",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx src",
    "updateIconfont": "bash -c 'cat $0 | fgrep -A3 .icon- | sed -E '\"'\"' s/\\.icon-(.*):before /:global(.pyreneIcon-\\1:before)/ '\"'\"' >  src/styles/iconfont.css; cp $1 src/fonts/pyrene-iconfont.woff;'"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.50",
    "@types/react-css-modules": "^4.6.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "autoprefixer": "^9.8.6",
    "@types/react-svg-inline": "^2.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.0.0",
    "babel-plugin-react-docgen": "^4.2.0",
    "babel-plugin-typescript-to-proptypes": "^1.4.1",
    "copy-webpack-plugin": "^6.2.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-pyrene": "^1.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^25.1.0",
    "jest-bamboo-formatter": "^1.0.1",
    "mini-css-extract-plugin": "^0.11.3",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.31",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^7.0.35",
    "postcss-custom-properties": "^9.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "release-it": "^14.0.3",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^4.2.2",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react-select": "^3.1.0",
    "react-svg-inline": "^2.1.1",
    "react-table": "^6.8.6",
    "react-tiny-popover": "^4.0.0",
    "react-window": "^1.8.5",
    "strip-sourcemap-loader": "0.0.1",
    "uniqid": "^5.0.3"
  },
  "peerDependencies": {
    "react": ">=16.9.0 <17.0.0",
    "react-dom": ">=16.9.0 <17.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26"
  }
}
